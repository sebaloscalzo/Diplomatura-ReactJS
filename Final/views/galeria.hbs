<div style="width: 100%; overflow-y:auto; padding: 25px">

<h2 class="title">| {{ title }} |</h2>

  <div class="container"><div class="row">

    {{#each productos}}

      <div class="col-sm-6 col-md-5 col-lg-5 col-xl-4 col-xxl-3" style="text-align: -webkit-center;">
        <div class="thumb">
          <div class="card-img" style="background-image: url('../images/{{imagen}}.jpg')"></div>
        </div>
      </div>

    {{/each}}

      <div class="col-sm-6 col-md-5 col-lg-5 col-xl-4 col-xxl-3" style="text-align: -webkit-center;">
        <div class="thumb">
          <div class="card-logo"></div>
        </div>
      </div>

  </div></div>

</div>